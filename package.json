{
    "name": "databind",
    "displayName": "Databind",
    "description": "Databind Syntax Highlighter",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "databind",
            "aliases": ["Databind", "databind"],
            "extensions": [".databind"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "databind",
            "scopeName": "source.databind",
            "path": "./syntaxes/databind.tmLanguage.json"
        }],
        "themes": [
            {
                "label": "Databind Syntax Highlighter",
                "uiTheme": "vs-dark",
                "path": "./themes/Databind-color-theme.json"
            }
        ]
    }
}